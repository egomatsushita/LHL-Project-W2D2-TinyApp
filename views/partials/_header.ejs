<h1>TinyURL App</h1>

<% if(user) { %>
  <form method="POST" action="/logout">
    <label><h2>Welcome, <%= user.email %></h2></label>
    <input type="submit" value="Logout">
  </form>
<% } else { %>
  <a href="/login"><input type="button" value="Login"></a>
  <a href="/register"><input type="button" value="Register"></a>

<% } %>
